{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    // "noEmit": false,
    "esModuleInterop": true,
    "module": "ES6",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react",
    "incremental": true,
    "baseUrl": ".",
    "outDir": "./dist"
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
